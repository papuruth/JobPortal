    "axios": "^0.19.2",
    "babel-loader": "^8.1.0",
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "connect-busboy": "0.0.2",
    "connect-mongo": "^3.2.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "enum": "^2.5.0",
    "express": "^4.16.4",
    "express-session": "^1.16.1",
    "firebase-admin": "^9.2.0",
    "http": "0.0.0",
    "moment": "^2.24.0",
    "mongoose": "^5.7.5",
    "morgan": "^1.8.2",
    "multer": "^1.4.1",
    "nodemailer": "^6.4.5",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "passport": "^0.3.2",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth": "^2.0.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-local": "^1.0.0",
    "randomstring": "^1.1.5",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "socket.io": "^2.5.0",
    "uuid": "^3.3.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "scripts": {
    "start": "npm run prod:build-server && npm-run-all --parallel prod:*",
    "prod:build-server": "cross-env NODE_ENV=production webpack --config webpack.server.js",
    "prod:build-server-watch": "cross-env NODE_ENV=production webpack --config webpack.server.js --watch",
    "prod:server": "nodemon --watch server-build --exec \"node server-build/bundle.js\"",
    "dev": "npm run build-server && npm-run-all --parallel dev:*",
    "prod:client": "cd client && npm start",
    "build-client": "cd client && npm run build",
    "build-server": "cross-env NODE_ENV=development webpack --config webpack.server.js",
    "dev:build-server": "cross-env NODE_ENV=development webpack --config webpack.server.js --watch",
    "dev:server": "nodemon --watch server-build --exec \"node server-build/bundle.js\"",
    "heroku-postbuild": "cd client && npm install --production=false && npm run build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^4.1.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "1.19.1"
  }
}
